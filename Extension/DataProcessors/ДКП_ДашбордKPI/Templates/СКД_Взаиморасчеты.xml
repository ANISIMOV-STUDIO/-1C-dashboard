		<dataSource>ИсточникДанных1</dataSource>
		<query>
////////////////////////////////////////////////////////////////////////////////
// 1. Дебиторка (остаток + просрочка)
ВЫБРАТЬ
    Ост.АналитикаУчетаПоПартнерам.Организация        КАК Организация,
    Ост.АналитикаУчетаПоПартнерам.Партнер            КАК Контрагент,

    СУММА(Ост.СуммаОстаток)                          КАК ДебиторкаВсего,

    СУММА(
        ВЫБОР
            КОГДА Дог.Ссылка ЕСТЬ НЕ NULL
                 И Дог.СрокОплаты &lt; &amp;КонецПериода
              ТОГДА Ост.СуммаОстаток
              ИНАЧЕ 0
        КОНЕЦ)                                        КАК ПросроченнаяДебиторка

ПОМЕСТИТЬ ВТ_Дебиторка
ИЗ
    РегистрНакопления.РасчетыСКлиентами.Остатки(&amp;КонецПериода) КАК Ост
        ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов КАК Дог
            ПО Ост.ОбъектРасчетов = Дог.Ссылка
ГДЕ
    Ост.СуммаОстаток &gt; 0

СГРУППИРОВАТЬ ПО
    Ост.АналитикаУчетаПоПартнерам.Организация,
    Ост.АналитикаУчетаПоПартнерам.Партнер;


////////////////////////////////////////////////////////////////////////////////
// 2. Кредиторка (остаток + просрочка)
ВЫБРАТЬ
    Ост.АналитикаУчетаПоПартнерам.Организация     КАК Организация,
    Ост.АналитикаУчетаПоПартнерам.Партнер         КАК Контрагент,

    СУММА(Ост.СуммаОстаток)                       КАК КредиторкаВсего,

    СУММА(
        ВЫБОР
            КОГДА  Дог.Ссылка ЕСТЬ НЕ NULL
                  И Дог.СрокОплаты &lt; &amp;КонецПериода
                ТОГДА Ост.СуммаОстаток
            ИНАЧЕ 0
        КОНЕЦ)                                    КАК ПросроченнаяКредиторка

ПОМЕСТИТЬ ВТ_Кредиторка
ИЗ
    РегистрНакопления.РасчетыСПоставщиками.Остатки(&amp;КонецПериода) КАК Ост
        ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов КАК Дог
            ПО Ост.ОбъектРасчетов = Дог.Ссылка
ГДЕ
    Ост.СуммаОстаток &gt; 0

СГРУППИРОВАТЬ ПО
    Ост.АналитикаУчетаПоПартнерам.Организация,
    Ост.АналитикаУчетаПоПартнерам.Партнер;


////////////////////////////////////////////////////////////////////////////////
// 3. Сводная таблица
ВЫБРАТЬ
    ЕСТЬNULL(Деб.Организация, Кр.Организация)   КАК Организация,
    ЕСТЬNULL(Деб.Контрагент,  Кр.Контрагент)    КАК Контрагент,

    ЕСТЬNULL(Деб.ДебиторкаВсего,        0)      КАК ДебиторкаВсего,
    ЕСТЬNULL(Кр.КредиторкаВсего,        0)      КАК КредиторкаВсего,

    ЕСТЬNULL(Деб.ПросроченнаяДебиторка, 0)      КАК ПросроченнаяДебиторка,
    ЕСТЬNULL(Кр.ПросроченнаяКредиторка, 0)      КАК ПросроченнаяКредиторка
ИЗ
    ВТ_Дебиторка КАК Деб
        ПОЛНОЕ СОЕДИНЕНИЕ ВТ_Кредиторка КАК Кр
            ПО Деб.Организация = Кр.Организация
           И Деб.Контрагент   = Кр.Контрагент
;


</query>
