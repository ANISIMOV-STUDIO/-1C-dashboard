		<dataSource>ИсточникДанных1</dataSource>
		<query>
// ───────────────────────────── 1. Средняя себестоимость на дату
ВЫБРАТЬ
    Ост.АналитикаУчетаНоменклатуры               КАК АналитикаНоменклатуры,
    Ост.Организация                              КАК Организация,
    ВЫБОР
        КОГДА СУММА(Ост.КоличествоОстаток) = 0
            ТОГДА 0
        ИНАЧЕ
            (СУММА(
                 Ост.СтоимостьБезНДСОстаток
               + Ост.ДопРасходыБезНДСОстаток
               + Ост.ПостатейныеПостоянныеБезНДСОстаток
               + Ост.ПостатейныеПеременныеБезНДСОстаток
               + Ост.ТрудозатратыОстаток)
            ) / СУММА(Ост.КоличествоОстаток)
    КОНЕЦ                                         КАК СредняяСебестоимость
ПОМЕСТИТЬ ВТ_СС_Сырые            // ─── уникальный псевдоним
ИЗ
    РегистрНакопления.СебестоимостьТоваров.Остатки(&amp;КонецПериода) КАК Ост
СГРУППИРОВАТЬ ПО
    Ост.АналитикаУчетаНоменклатуры,
    Ост.Организация;

// ───────────────────────────── 2. Привязка к номенклатуре
ВЫБРАТЬ
    Сырые.АналитикаНоменклатуры.Номенклатура      КАК Номенклатура,
    Сырые.Организация                             КАК Организация,
    Сырые.СредняяСебестоимость                    КАК СредняяСебестоимость
ПОМЕСТИТЬ ВТ_СС                   // ─── один-единственный alias «СС»
ИЗ
    ВТ_СС_Сырые КАК Сырые;

// ───────────────────────────── 3. Физические остатки
ВЫБРАТЬ
    Ост.Номенклатура              КАК Номенклатура,
    Ост.Склад                     КАК Склад,
    Ост.ВНаличииОстаток           КАК Количество
ПОМЕСТИТЬ ВТ_ФизОст
ИЗ
    РегистрНакопления.ТоварыНаСкладах.Остатки(&amp;КонецПериода) КАК Ост;

// ───────────────────────────── 4. Стоимостьные остатки
ВЫБРАТЬ
    Физ.Склад                                       КАК Склад,
    СС.Организация                                  КАК Организация,
    Физ.Номенклатура                                КАК Номенклатура,
    ЕСТЬNULL(Физ.Количество, 0)                     КАК КоличествоОстаток,
    ЕСТЬNULL(Физ.Количество, 0) * СС.СредняяСебестоимость  КАК СтоимостьОстаток
ПОМЕСТИТЬ ВТ_Ост
ИЗ
    ВТ_ФизОст КАК Физ
        ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СС КАК СС
            ПО СС.Номенклатура = Физ.Номенклатура;

// ─── блок 5: себестоимость продаж (финальная версия для MVP)
ВЫБРАТЬ
    Обороты.Организация                                  КАК Организация,
    Обороты.АналитикаУчетаНоменклатуры.Номенклатура      КАК Номенклатура,
    -1 * СУММА(
         Обороты.СтоимостьБезНДСОборот
       + Обороты.ДопРасходыБезНДСОборот
       + Обороты.ПостатейныеПостоянныеБезНДСОборот
       + Обороты.ПостатейныеПеременныеБезНДСОборот
       + Обороты.ТрудозатратыОборот)                     КАК СебестоимостьПродаж
ПОМЕСТИТЬ ВТ_СП
ИЗ
    РегистрНакопления.СебестоимостьТоваров.Обороты(&amp;НачалоПериода,
                                                   &amp;КонецПериода) КАК Обороты
ГДЕ
    Обороты.СтоимостьБезНДСОборот &lt; 0            // расход
СГРУППИРОВАТЬ ПО
    Обороты.Организация,
    Обороты.АналитикаУчетаНоменклатуры.Номенклатура;


// ───────────────────────────── 6. Отгрузка по заказам
ВЫБРАТЬ
    РеалТов.ЗаказКлиента                КАК ЗаказКлиента,
    РеалТов.Номенклатура                КАК Номенклатура,
    СУММА(РеалТов.Количество)           КАК КоличествоОтгружено
ПОМЕСТИТЬ ВТ_Отгруз
ИЗ
    Документ.РеализацияТоваровУслуг.Товары КАК РеалТов
ГДЕ
    РеалТов.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
    И РеалТов.ЗаказКлиента ЕСТЬ НЕ NULL
СГРУППИРОВАТЬ ПО
    РеалТов.ЗаказКлиента,
    РеалТов.Номенклатура;

// ───────────────────────────── 7. Сопоставление заказ ↔ отгрузка
ВЫБРАТЬ
    ЗаказТов.Ссылка.Организация         КАК Организация,
    ЗаказТов.Номенклатура               КАК Номенклатура,
    ЗаказТов.Ссылка.Склад               КАК Склад,
    ВЫБОР
        КОГДА ЗаказТов.Количество = 0
            ТОГДА 1
        КОГДА ЕСТЬNULL(Отгруз.КоличествоОтгружено, 0) >= ЗаказТов.Количество
            ТОГДА 1
        ИНАЧЕ 0
    КОНЕЦ                                КАК Выполнен
ПОМЕСТИТЬ ВТ_ЗаказДет
ИЗ
    Документ.ЗаказКлиента.Товары КАК ЗаказТов
        ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Отгруз КАК Отгруз
            ПО   ЗаказТов.Ссылка      = Отгруз.ЗаказКлиента
             И   ЗаказТов.Номенклатура = Отгруз.Номенклатура
ГДЕ
    ЗаказТов.Ссылка.Проведен
    И ЗаказТов.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода;

// ───────────────────────────── 8. KPI по выполнению заказов
ВЫБРАТЬ
    Дет.Организация                     КАК Организация,
    Дет.Номенклатура                    КАК Номенклатура,
    Дет.Склад                           КАК Склад,
    ВЫБОР
        КОГДА КОЛИЧЕСТВО(Дет.Выполнен) = 0
            ТОГДА 100
        ИНАЧЕ СУММА(Дет.Выполнен) * 100.0 / КОЛИЧЕСТВО(Дет.Выполнен)
    КОНЕЦ                                КАК КоэффициентВыполненияЗаказов
ПОМЕСТИТЬ ВТ_Заказы
ИЗ
    ВТ_ЗаказДет КАК Дет
СГРУППИРОВАТЬ ПО
    Дет.Организация,
    Дет.Номенклатура,
    Дет.Склад;

// ───────────────────────────── 9. Финальный вывод
ВЫБРАТЬ
    Ост.Организация,
    Ост.Склад,
    Ост.Номенклатура,
    Ост.Номенклатура.Родитель              КАК ГруппаНоменклатуры,
    Ост.КоличествоОстаток,
    Ост.СтоимостьОстаток,
    ЕСТЬNULL(СП.СебестоимостьПродаж, 0)     КАК СебестоимостьПродаж,
    // ——— Оборачиваемость
    ВЫБОР
        КОГДА   Ост.СтоимостьОстаток = 0
              ИЛИ (РАЗНОСТЬДАТ(&amp;НачалоПериода, &amp;КонецПериода, ДЕНЬ) + 1) = 0
            ТОГДА 0
        ИНАЧЕ   ЕСТЬNULL(СП.СебестоимостьПродаж, 0) / Ост.СтоимостьОстаток
    КОНЕЦ                                 КАК ОборачиваемостьЗапасов,
    // ——— Дни запаса
    ВЫБОР
        КОГДА   ЕСТЬNULL(СП.СебестоимостьПродаж, 0) = 0
              ИЛИ (РАЗНОСТЬДАТ(&amp;НачалоПериода, &amp;КонецПериода, ДЕНЬ) + 1) = 0
            ТОГДА 999
        ИНАЧЕ   Ост.СтоимостьОстаток /
               (ЕСТЬNULL(СП.СебестоимостьПродаж, 0) /
                (РАЗНОСТЬДАТ(&amp;НачалоПериода, &amp;КонецПериода, ДЕНЬ) + 1))
    КОНЕЦ                                 КАК ДнейЗапаса,
    ЕСТЬNULL(Заказы.КоэффициентВыполненияЗаказов, 100)   КАК КоэффициентВыполненияЗаказов
ИЗ
    ВТ_Ост КАК Ост
        ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СП КАК СП
            ПО   СП.Организация  = Ост.Организация
             И   СП.Номенклатура = Ост.Номенклатура
        ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Заказы КАК Заказы
            ПО   Заказы.Организация = Ост.Организация
             И   Заказы.Номенклатура = Ост.Номенклатура
             И   Заказы.Склад        = Ост.Склад;
</query>

