		<dataSource>ИсточникДанных1</dataSource>
		<query>////////////////////////////////////////////////////////////////////////////////
// 1. Остатки на начало

ВЫБРАТЬ
    Остатки.Организация    КАК Организация,
    Остатки.БанковскийСчет КАК Счет,
    СУММА(Остатки.СуммаОстаток) КАК НачальныйОстаток
ПОМЕСТИТЬ ВТ_НачальныеОстатки
ИЗ
    РегистрНакопления.ДенежныеСредстваБезналичные.Остатки(&amp;НачалоПериода) КАК Остатки
СГРУППИРОВАТЬ ПО
    Остатки.Организация,
    Остатки.БанковскийСчет

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
    Остатки.Организация  КАК Организация,
    Остатки.Касса       КАК Счет,
    СУММА(Остатки.СуммаОстаток) КАК НачальныйОстаток
ИЗ
    РегистрНакопления.ДенежныеСредстваНаличные.Остатки(&amp;НачалоПериода) КАК Остатки
СГРУППИРОВАТЬ ПО
    Остатки.Организация,
    Остатки.Касса
;

////////////////////////////////////////////////////////////////////////////////
// 2. Обороты за период (безналичные + наличные) — без детализации по дате
ВЫБРАТЬ
    Безн.Организация             КАК Организация,
    Безн.БанковскийСчет          КАК Счет,
    СУММА(Безн.СуммаПриход)      КАК Поступления,
    СУММА(Безн.СуммаРасход)      КАК Платежи,
    "Операционный"               КАК ВидПотока
ПОМЕСТИТЬ ВТ_Обороты
ИЗ
    РегистрНакопления.ДенежныеСредстваБезналичные.Обороты(&amp;НачалоПериода,&amp;КонецПериода) КАК Безн
СГРУППИРОВАТЬ ПО
    Безн.Организация,
    Безн.БанковскийСчет

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
    Нал.Организация             КАК Организация,
    Нал.Касса                   КАК Счет,
    СУММА(Нал.СуммаПриход)      КАК Поступления,
    СУММА(Нал.СуммаРасход)      КАК Платежи,
    "Операционный"              КАК ВидПотока
ИЗ
    РегистрНакопления.ДенежныеСредстваНаличные.Обороты(&amp;НачалоПериода,&amp;КонецПериода) КАК Нал
СГРУППИРОВАТЬ ПО
    Нал.Организация,
    Нал.Касса
;



////////////////////////////////////////////////////////////////////////////////
// 3. Остатки на конец

ВЫБРАТЬ
    Остатки.Организация,
    Остатки.БанковскийСчет         КАК Счет,
    СУММА(Остатки.СуммаОстаток)    КАК КонечныйОстаток
ПОМЕСТИТЬ ВТ_КонечныеОстатки
ИЗ
    РегистрНакопления.ДенежныеСредстваБезналичные.Остатки(&amp;КонецПериода) КАК Остатки
СГРУППИРОВАТЬ ПО
    Остатки.Организация,
    Остатки.БанковскийСчет

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
    Остатки.Организация,
    Остатки.Касса          КАК Счет,
    СУММА(Остатки.СуммаОстаток)    КАК КонечныйОстаток
ИЗ
    РегистрНакопления.ДенежныеСредстваНаличные.Остатки(&amp;КонецПериода) КАК Остатки
СГРУППИРОВАТЬ ПО
    Остатки.Организация,
    Остатки.Касса
;

////////////////////////////////////////////////////////////////////////////////
// 4. Финальный срез для дашборда
ВЫБРАТЬ
    Обороты.Организация,
    Обороты.Счет,

    ЕСТЬNULL(Нач.НачальныйОстаток,0)                                    КАК НачальныйОстаток,
    СУММА(Обороты.Поступления)                                          КАК Поступления,
    СУММА(Обороты.Платежи)                                              КАК Платежи,
    ЕСТЬNULL(Кон.КонечныйОстаток,0)                                     КАК КонечныйОстаток,

    СУММА(ВЫБОР КОГДА Обороты.ВидПотока = "Операционный"
              ТОГДА Обороты.Поступления - Обороты.Платежи
              ИНАЧЕ 0 КОНЕЦ)                                            КАК ОперационныйПоток,

    СУММА(ВЫБОР КОГДА Обороты.ВидПотока = "Инвестиционный"
              ТОГДА Обороты.Поступления - Обороты.Платежи
              ИНАЧЕ 0 КОНЕЦ)                                            КАК ИнвестиционныйПоток,

    СУММА(ВЫБОР КОГДА Обороты.ВидПотока = "Финансовый"
              ТОГДА Обороты.Поступления - Обороты.Платежи
              ИНАЧЕ 0 КОНЕЦ)                                            КАК ФинансовыйПоток,

    СУММА(Обороты.Поступления - Обороты.Платежи)                         КАК СвободныйДенежныйПоток,

    ВЫБОР
        КОГДА СУММА(Обороты.Платежи) = 0
            ТОГДА 0
        ИНАЧЕ ЕСТЬNULL(Нач.НачальныйОстаток,0) / СУММА(Обороты.Платежи)
    КОНЕЦ                                                                КАК КоэффициентДенежнойЛиквидности

ИЗ
    ВТ_Обороты КАК Обороты
        ЛЕВОЕ СОЕДИНЕНИЕ ВТ_НачальныеОстатки КАК Нач
            ПО   Обороты.Организация = Нач.Организация
             И   Обороты.Счет        = Нач.Счет
        ЛЕВОЕ СОЕДИНЕНИЕ ВТ_КонечныеОстатки КАК Кон
            ПО   Обороты.Организация = Кон.Организация
             И   Обороты.Счет        = Кон.Счет

СГРУППИРОВАТЬ ПО
    Обороты.Организация,
    Обороты.Счет
;

</query>
